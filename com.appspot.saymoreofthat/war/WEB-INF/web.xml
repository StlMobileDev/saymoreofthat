<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ListEventsServlet</servlet-name>
		<servlet-class>com.appspot.saymoreofthat.ListEventsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddEventServlet</servlet-name>
		<servlet-class>com.appspot.saymoreofthat.AddEventServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ShowEventServlet</servlet-name>
		<servlet-class>com.appspot.saymoreofthat.ShowEventServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>VoteServlet</servlet-name>
		<servlet-class>com.appspot.saymoreofthat.VoteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddEventServlet</servlet-name>
		<url-pattern>/events/add</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>VoteServlet</servlet-name>
		<url-pattern>/events/vote</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ListEventsServlet</servlet-name>
		<url-pattern>/events/list</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ShowEventServlet</servlet-name>
		<url-pattern>/events/show</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
